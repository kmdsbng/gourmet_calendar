listStation[1491] = '西小路通';
listStation[1492] = '木津屋橋通';



function startSuggest() {
	if (document.getElementById("txtKeyword")) {
		new Suggest.Local(
			"txtKeyword",
			"suggest",
			listKeyword,
			{dispMax: 10, interval:1000}
		);
	}
	if (document.getElementById("txtKeyword2")) {
		new Suggest.Local(
			"txtKeyword2",
			"suggest2",
			listKeyword,
			{dispMax: 10, interval:1000}
		);
	}
	if (document.getElementById("txtStation")) {
		new Suggest.Local(
			"txtStation",
			"suggest3",
			listStation,
			{dispMax: 10, interval:1000}
		);
	}
}
window.addEventListener ?
  window.addEventListener('load', startSuggest, false) :
  window.attachEvent('onload', startSuggest);


</script>

